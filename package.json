{
  "name": "mern-shop",
  "version": "1.0.0",
  "description": "MERN shooping cart ",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gardianos/MERN-SHOP.git"
  },
  "keywords": [
    "nodejs",
    "and",
    "react",
    "js",
    "with",
    "mongodb",
    "shooping",
    "cart"
  ],
  "author": "Gardian Memeti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gardianos/MERN-SHOP/issues"
  },
  "homepage": "https://github.com/gardianos/MERN-SHOP#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.15"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
